<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>loginservlet</servlet-name>
        <servlet-class>publicserviceforum.loginservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signupservlet</servlet-name>
        <servlet-class>publicserviceforum.signupservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CityServlet</servlet-name>
        <servlet-class>publicserviceforum.CityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SectionServlet</servlet-name>
        <servlet-class>publicserviceforum.SectionServlet</servlet-class>
    </servlet>
    <servlet>
